<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-03-02T15:32:50Z</date>
    <groups>
        <group>
            <name>Templates/Databases</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>template_oracle</template>
            <name>Template Oracle</name>
            <groups>
                <group>
                    <name>Templates/Databases</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>ASM Diskgroup</name>
                </application>
                <application>
                    <name>Query</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>asm.diskgroup.info</name>
                    <key>asm.diskgroup.info</key>
                    <delay>10m</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>ASM Diskgroup</name>
                        </application>
                        <application>
                            <name>Query</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>ASM Diskgroup Discovery</name>
                    <key>asm.diskgroup.discovery</key>
                    <delay>1d</delay>
                    <lifetime>1w</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>ASM Diskgroup: Espacio libre en {#DISKGROUP}</name>
                            <type>DEPENDENT</type>
                            <key>asm.diskgroup.free[{#DISKGROUP}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Query</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..{#DISKGROUP}['FREE_MB']</params>
                                </step>
                                <step>
                                    <type>REGEX</type>
                                    <params>[0-9]+
\0</params>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1048576</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>asm.diskgroup.info</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>ASM Diskgroup: Espacio total en {#DISKGROUP}</name>
                            <type>DEPENDENT</type>
                            <key>asm.diskgroup.total[{#DISKGROUP}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Query</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..{#DISKGROUP}.TOTAL_MB</params>
                                </step>
                                <step>
                                    <type>REGEX</type>
                                    <params>[0-9]+
\0</params>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1048576</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>asm.diskgroup.info</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>ASM Diskgroup: Espacio usado en {#DISKGROUP}</name>
                            <type>CALCULATED</type>
                            <key>asm.diskgroup.used[{#DISKGROUP}]</key>
                            <history>30d</history>
                            <units>B</units>
                            <params>last(&quot;asm.diskgroup.total[{#DISKGROUP}]&quot;) - last(&quot;asm.diskgroup.free[{#DISKGROUP}]&quot;)</params>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Query</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>({template_oracle:asm.diskgroup.free[{#DISKGROUP}].last()} / {template_oracle:asm.diskgroup.total[{#DISKGROUP}].last()} ) &lt;= 0.1</expression>
                            <name>Espacio libre en {#DISKGROUP} con menos del 1%</name>
                            <priority>DISASTER</priority>
                            <tags>
                                <tag>
                                    <tag>app</tag>
                                    <value>asm</value>
                                </tag>
                            </tags>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>({template_oracle:asm.diskgroup.free[{#DISKGROUP}].last()} / {template_oracle:asm.diskgroup.total[{#DISKGROUP}].last()} ) &lt;= 0.5</expression>
                            <name>Espacio libre en {#DISKGROUP} con menos del 5%</name>
                            <priority>HIGH</priority>
                            <dependencies>
                                <dependency>
                                    <name>Espacio libre en {#DISKGROUP} con menos del 1%</name>
                                    <expression>({template_oracle:asm.diskgroup.free[{#DISKGROUP}].last()} / {template_oracle:asm.diskgroup.total[{#DISKGROUP}].last()} ) &lt;= 0.1</expression>
                                </dependency>
                            </dependencies>
                            <tags>
                                <tag>
                                    <tag>app</tag>
                                    <value>asm</value>
                                </tag>
                            </tags>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>({template_oracle:asm.diskgroup.free[{#DISKGROUP}].last()} / {template_oracle:asm.diskgroup.total[{#DISKGROUP}].last()} ) &lt;= 0.10</expression>
                            <name>Espacio libre en {#DISKGROUP} con menos del 10%</name>
                            <priority>AVERAGE</priority>
                            <dependencies>
                                <dependency>
                                    <name>Espacio libre en {#DISKGROUP} con menos del 5%</name>
                                    <expression>({template_oracle:asm.diskgroup.free[{#DISKGROUP}].last()} / {template_oracle:asm.diskgroup.total[{#DISKGROUP}].last()} ) &lt;= 0.5</expression>
                                </dependency>
                            </dependencies>
                            <tags>
                                <tag>
                                    <tag>app</tag>
                                    <value>asm</value>
                                </tag>
                            </tags>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>ASM Diskgroup: Espacio en {#DISKGROUP}</name>
                            <type>STACKED</type>
                            <ymax_type_1>ITEM</ymax_type_1>
                            <ymax_item_1>
                                <host>template_oracle</host>
                                <key>asm.diskgroup.total[{#DISKGROUP}]</key>
                            </ymax_item_1>
                            <graph_items>
                                <graph_item>
                                    <color>1A7C11</color>
                                    <item>
                                        <host>template_oracle</host>
                                        <key>asm.diskgroup.used[{#DISKGROUP}]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>FFEE58</color>
                                    <item>
                                        <host>template_oracle</host>
                                        <key>asm.diskgroup.free[{#DISKGROUP}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Tablespace Discovery</name>
                    <key>tablespace.discovery</key>
                    <delay>1d</delay>
                    <lifetime>1w</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Tablespace {#SID} {#VERSION} - {#TABLESPACE}</name>
                            <key>tablespace.info[{#SID},{#TABLESPACE},{#VERSION}]</key>
                            <delay>1h</delay>
                            <history>7d</history>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Tablespace</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=99</expression>
                                    <name>Tablespace {#SID} - {#TABLESPACE} &gt;= 99%</name>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                    <dependencies>
                                        <dependency>
                                            <name>Tablespace {#SID} - {#TABLESPACE} &gt;=99.5%</name>
                                            <expression>{template_oracle:tablespace.info[{#SID},{#TABLESPACE},{#VERSION}].last()}&gt;=99.5</expression>
                                        </dependency>
                                    </dependencies>
                                    <tags>
                                        <tag>
                                            <tag>app</tag>
                                            <value>tablespace</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=99.5</expression>
                                    <name>Tablespace {#SID} - {#TABLESPACE} &gt;=99.5%</name>
                                    <priority>HIGH</priority>
                                    <tags>
                                        <tag>
                                            <tag>app</tag>
                                            <value>tablespace</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
