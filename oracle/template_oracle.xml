<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2021-04-14T23:30:36Z</date>
    <groups>
        <group>
            <name>Templates/Databases</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>template_oracle</template>
            <name>Template Oracle</name>
            <groups>
                <group>
                    <name>Templates/Databases</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>ASM Diskgroup</name>
                </application>
                <application>
                    <name>Oracle</name>
                </application>
                <application>
                    <name>Query</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Password expires user oracle</name>
                    <key>get_expiration_user[oracle]</key>
                    <delay>1h</delay>
                    <history>2d</history>
                    <trends>0</trends>
                    <status>DISABLED</status>
                    <value_type>FLOAT</value_type>
                    <units>d</units>
                    <applications>
                        <application>
                            <name>Oracle</name>
                        </application>
                    </applications>
                    <preprocessing>
                        <step>
                            <type>DISCARD_UNCHANGED</type>
                            <params/>
                        </step>
                    </preprocessing>
                    <triggers>
                        <trigger>
                            <expression>{last()}=-1</expression>
                            <name>Contraseña usuario &quot;oracle&quot; expira mañana</name>
                            <priority>AVERAGE</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                        <trigger>
                            <expression>{last()}&gt;=0</expression>
                            <name>Contraseña usuario &quot;oracle&quot; ha expirado!</name>
                            <priority>HIGH</priority>
                            <manual_close>YES</manual_close>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>ASM Diskgroup Discovery</name>
                    <key>asm.diskgroup.discovery</key>
                    <delay>1d</delay>
                    <lifetime>1w</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>ASM Diskgroup {#HOST}: Espacio libre en {#DISKGROUP}</name>
                            <type>DEPENDENT</type>
                            <key>asm.diskgroup.free[{#HOST},{#DISKGROUP}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Oracle</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$..{#DISKGROUP}['FREE_MB']</params>
                                </step>
                                <step>
                                    <type>REGEX</type>
                                    <params>[0-9]+
\0</params>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1048576</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>asm.diskgroup.info[{#HOST}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>ASM Diskgroup {#HOST}: Info</name>
                            <key>asm.diskgroup.info[{#HOST}]</key>
                            <delay>1h</delay>
                            <history>0</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Oracle</name>
                                </application>
                                <application>
                                    <name>Query</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>ASM Diskgroup {#HOST}: Espacio usado en {#DISKGROUP}</name>
                            <type>CALCULATED</type>
                            <key>asm.diskgroup.used[{#HOST},{#DISKGROUP}]</key>
                            <delay>1h</delay>
                            <history>30d</history>
                            <units>B</units>
                            <params>last(&quot;asm.diskgroup.total[{#HOST},{#DISKGROUP}]&quot;) - last(&quot;asm.diskgroup.free[{#HOST},{#DISKGROUP}]&quot;)</params>
                            <applications>
                                <application>
                                    <name>ASM Diskgroup</name>
                                </application>
                                <application>
                                    <name>Oracle</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>ASM Diskgroup: Espacio en {#DISKGROUP}</name>
                            <type>STACKED</type>
                            <graph_items>
                                <graph_item>
                                    <color>1A7C11</color>
                                    <item>
                                        <host>template_oracle</host>
                                        <key>asm.diskgroup.used[{#HOST},{#DISKGROUP}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>Tablespace Discovery</name>
                    <key>tablespace.discovery</key>
                    <delay>1d</delay>
                    <lifetime>1w</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Tablespace {#SID} - {#TABLESPACE}</name>
                            <key>tablespace.info[{#SID},{#TABLESPACE}]</key>
                            <delay>1h</delay>
                            <history>7d</history>
                            <value_type>FLOAT</value_type>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>Oracle</name>
                                </application>
                            </applications>
                            <application_prototypes>
                                <application_prototype>
                                    <name>Tablespace</name>
                                </application_prototype>
                            </application_prototypes>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=99</expression>
                                    <name>Tablespace {#SID} - {#TABLESPACE} &gt;= 99%</name>
                                    <priority>AVERAGE</priority>
                                    <manual_close>YES</manual_close>
                                    <dependencies>
                                        <dependency>
                                            <name>Tablespace {#SID} - {#TABLESPACE} &gt;=99.5%</name>
                                            <expression>{template_oracle:tablespace.info[{#SID},{#TABLESPACE}].last()}&gt;=99.5</expression>
                                        </dependency>
                                    </dependencies>
                                    <tags>
                                        <tag>
                                            <tag>app</tag>
                                            <value>tablespace</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                                <trigger_prototype>
                                    <expression>{last()}&gt;=99.5</expression>
                                    <name>Tablespace {#SID} - {#TABLESPACE} &gt;=99.5%</name>
                                    <priority>HIGH</priority>
                                    <tags>
                                        <tag>
                                            <tag>app</tag>
                                            <value>tablespace</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
